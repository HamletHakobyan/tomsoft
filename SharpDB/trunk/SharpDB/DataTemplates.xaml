<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:SharpDB.ViewModel;assembly=SharpDB.ViewModel"
                    xmlns:v="clr-namespace:SharpDB.View">

    <DataTemplate DataType="{x:Type vm:WorksheetViewModel}">
        <v:WorksheetView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DatabaseViewModel}">
        <v:DatabaseView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DatabaseManagerViewModel}">
        <v:DatabaseManagerView />
    </DataTemplate>

</ResourceDictionary>