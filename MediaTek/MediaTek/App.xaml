<Application x:Class="MediaTek.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="XamlResources/GlobalResources.xaml" />
                <ResourceDictionary Source="XamlResources/ItemMenus.xaml" />
                <ResourceDictionary Source="XamlResources/ControlTemplates.xaml" />
                <ResourceDictionary Source="XamlResources/Styles.xaml" />
                <ResourceDictionary Source="XamlResources/DataTemplates.xaml" />
                <ResourceDictionary Source="Filtering/FilterTemplates.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
